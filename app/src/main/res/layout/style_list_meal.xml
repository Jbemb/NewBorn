<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".activity.change.ChangeListActivity"
    android:paddingBottom="5dp">


    <TextView
        android:id="@+id/tv_dateItem"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_margin="30dp"
        android:layout_marginLeft="44dp"
        android:layout_marginTop="20dp"
        android:text="DATE"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tv_timeItem"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_margin="30dp"
        android:layout_marginLeft="40dp"
        android:layout_marginTop="20dp"
        android:text="HEURE"
        app:layout_constraintLeft_toRightOf="@id/tv_dateItem"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tv_quantityItem"
        android:layout_width="78dp"
        android:layout_height="19dp"
        android:layout_margin="30dp"
        android:layout_marginLeft="32dp"
        android:layout_marginTop="28dp"
        android:text="QUANTITE"
        android:visibility="invisible"
        app:layout_constraintLeft_toRightOf="@id/tv_timeItem"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tv_breastItem"
        android:layout_width="28dp"
        android:layout_height="18dp"
        android:layout_margin="30dp"
        android:layout_marginLeft="24dp"
        android:layout_marginTop="28dp"
        android:text="SEIN"
        android:visibility="invisible"
        app:layout_constraintLeft_toRightOf="@id/tv_quantityItem"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:id="@+id/ib_modifyMeal"
        android:layout_width="20dp"
        android:layout_height="20dp"
        android:layout_marginLeft="44dp"
        android:layout_marginTop="28dp"
        android:background="@drawable/edit"
        android:scaleType="fitXY"
        app:layout_constraintLeft_toRightOf="@id/tv_breastItem"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:id="@+id/ib_deleteMeal"
        android:layout_width="20dp"
        android:layout_height="20dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="28dp"
        android:scaleType="fitXY"
        android:background="@drawable/delete"
        app:layout_constraintLeft_toRightOf="@id/ib_modifyMeal"
        app:layout_constraintTop_toTopOf="parent"
        />

</androidx.constraintlayout.widget.ConstraintLayout>
